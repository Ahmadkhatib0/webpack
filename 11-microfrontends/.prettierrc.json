{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "htmlWhitespaceSensitivity": "ignore",
  "bracketSameLine": true,
  "printWidth": 100,
  "parser": "vue",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "arrowParens": "always",
        "bracketSpacing": true,
        "embeddedLanguageFormatting": "auto",
        "endOfLine": "lf",
        "filepath": "undefined",
        "htmlWhitespaceSensitivity": "css",
        "insertPragma": false,
        "jsxBracketSameLine": false,
        "jsxSingleQuote": true ,
        "parser": "babel",
        "printWidth": 90,
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "requirePragma": false,
        "tabWidth": 2,
        "trailingComma": "es5",
        "useTabs": false,
        "vueIndentScriptAndStyle": false
      }
    }
  ]
}